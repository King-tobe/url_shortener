    "import pyshorteners\n",
    "\n",
    "long_url  = input(\"Enter url to shorten: \")\n",
    "\n",
    "url_convert = pyshorteners.Shortener()\n",
    "\n",
    "new_url = url_convert.tinyurl.short(long_url)\n",
    "\n",
    "print(\"The shortened url: \" + new_url)"
   ]
